{
  "name": "@traveloka/react-schema-form",
  "version": "3.1.1",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "lint": "tslint --type-check -p tsconfig.json src/*.tsx && tslint --type-check -p tsconfig.json tests/*.tsx",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "test:ci": "yarn lint && yarn test:coverage -i --reporters jest-simple-dot-reporter jest-junit",
    "test:html": "nyc npm test && nyc report --reporter=html",
    "prebuild": "npm run clean",
    "clean": "rimraf lib/*",
    "build": "tsc -p .",
    "watch": "tsc -w -p .",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@traveloka/react-schema": "^3.1.1",
    "@traveloka/validation": "^3.1.1",
    "create-react-context": "^0.2.3"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/react": "^16.4.8",
    "@types/react-dom": "^16.0.7",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-junit": "^5.1.0",
    "jest-simple-dot-reporter": "^1.0.2",
    "nyc": "^13.0.1",
    "react": "^16.3.1",
    "react-test-renderer": "^16.3.2",
    "ts-jest": "23.10.1",
    "tslint": "5.11.0",
    "typescript": "^3.0.1"
  },
  "license": "MIT",
  "jest": {
    "notify": true,
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testURL": "http://localhost",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
